{
    "version": 2,
    "routes": [
      { "src": "^/api/(.*)$", "dest": "api/index.js" },
      { "src": "^(?!/api/).*", "dest": "dist/index.html" }
    ],
    "headers": [
      {
        "source": "/(.*)",
        "headers": [
          { "key": "Cross-Origin-Opener-Policy", "value": "same-origin-allow-popups" }
        ]
      }
    ]
  }
  