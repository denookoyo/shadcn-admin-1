{
    "version": 2,
    "framework": null,
    "buildCommand": "pnpm run build",
    "outputDirectory": "dist",
    "rewrites": [
      { "source": "/api/(.*)", "destination": "/api/index" },
      { "source": "/(.*)", "destination": "/" }
    ],
    "headers": [
      {
        "source": "/(.*)",
        "headers": [
          { "key": "Cross-Origin-Opener-Policy", "value": "same-origin-allow-popups" }
        ]
      }
    ]
  }
  